Proceso Multipl
    Definir num1, num2 Como Entero;
    Escribir "Ingrese el primer número:";
    Leer num1;
    Escribir "Ingrese el segundo número:";
    Leer num2;
    
    Si num1 mod num2 == 0 Entonces
        Escribir num1, " es múltiplo de ", num2;
    Sino
        Escribir num1, " no es múltiplo de ", num2;
    FinSi;
FinProceso
